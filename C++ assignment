#include<iostream>
using namespace std;

int main()
{
    string name ;
    name="Arslan Sadar \n";
    string id;
    id="bsf2205762\n";
    cout<<name;
    cout <<id;
    int a,b;
    a=50;
    b=10;
    cout<<"a+b="<<a+b;
    cout<<"\na-b="<<a-b;
    cout<<"\na*b="<<a*b;
    cout<<"\na/b="<<a/b;
    cout<<"\na%b="<<a%b;
    return 0;
}
